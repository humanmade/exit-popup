{
	"$schema": "https://schemas.wp.org/trunk/block.json",
	"apiVersion": 3,
	"name": "hm/exit-popup",
	"version": "0.1.0",
	"title": "Exit Popup",
	"category": "widgets",
	"icon": "exit",
	"description": "Exit popup wrapper block",
	"example": {},
	"supports": {
		"html": false,
		"color": {
			"background": true,
